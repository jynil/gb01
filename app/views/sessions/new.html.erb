<%= render 'shared/title', title: 'User Log In'%>

  <%= form_tag '/login' do |f| %>

    <%= label_tag :name %>
    <br>
    <%= text_field_tag :name, params[:name] %>
  
    <br>
    <%= label_tag :password %>
    <br>
    <%= password_field_tag :password, params[:password] %>

  <br>
  <br>
  <br>

  <%= submit_tag "Login", class: "btn btn-success" %>
  <% end %>